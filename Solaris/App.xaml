<Application x:Class="Solaris.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Solaris"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Themes/IconButtonTheme.xaml"/>
                <ResourceDictionary Source="Themes/ContactCard.xaml"/>
                

            </ResourceDictionary.MergedDictionaries>

            <DataTemplate DataType="{x:Type viewModel:SignUpFormViewModel}">
                <view:SignUpView/>






        </ResourceDictionary>


    </Application.Resources>
</Application>
